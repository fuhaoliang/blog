{
  "presets": [
    ["@babel/preset-env",{
        "modules": false,
        "targets":{
            "browers":[">1%","last 2 versions"]
        },
        "useBuiltIns":"usage",
        "corejs": 3
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-modules-commonjs",
    [
        "import",
        {
            "libraryName": "ant-design-vue",
            "libraryDirectory": "lib",
            "style": "css"              
        }
    ]
]
}